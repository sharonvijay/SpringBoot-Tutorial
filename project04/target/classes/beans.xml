<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation=" http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<!-- bean definitions here -->

	<bean id="MathCheatObjectValue"
		class="com.spring.project04.MathCheat"></bean>
	<bean id="ScienceCheatObjectValue"
		class="com.spring.project04.ScienceCheat"></bean>

	<bean id="stu" class="com.spring.project04.Student">
		<property name="cheat" ref="ScienceCheatObjectValue">
		</property>
	</bean>

</beans>